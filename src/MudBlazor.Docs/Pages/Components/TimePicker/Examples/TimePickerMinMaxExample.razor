@namespace MudBlazor.Docs.Examples

<MudTimePicker Label="24 hours" MinTime="new TimeSpan(9 , 30, 0)" MaxTime="new TimeSpan(17, 45, 0)" />

<MudTimePicker PickerVariant="PickerVariant.Static" AmPm="true" MinTime="new TimeSpan(9 , 30, 0)" MaxTime="new TimeSpan(17, 45, 0)" />
<MudTimePicker PickerVariant="PickerVariant.Static" IsTimeDisabledFunc="@((TimeSpan ts) => (ts.Hours % 2 == 0) ? ts.Minutes >= 30 : ts.Minutes < 30)" />